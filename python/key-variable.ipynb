    "def con_temp(temp,unit):\n",
    "    \"\"\"This function converts temprature between celsius and fahrenheit\"\"\"\n",
    "    if unit == 'C':\n",
    "        return temp * 9/5 + 32\n",
    "    elif unit == \"F\":\n",
    "        return (temp-32)*5/9\n",
    "    else:\n",
    "        return None\n",
    "\n",
    "print(con_temp(25,\"C\"));\n",
    "print(con_temp(25,\"F\"));\n",
    "\n"
   ]
